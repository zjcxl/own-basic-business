{
  "name": "@own-team/own-basic-business",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "version": "0.0.626",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -rfwi"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^8.1.1",
    "bumpp": "^9.11.1",
    "eslint": "^9.25.1",
    "esno": "^4.8.0",
    "pnpm": "^9.15.9",
    "rimraf": "^5.0.10",
    "taze": "^18.7.1",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^6.3.2",
    "vitest": "^3.1.2"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}
