{
  "name": "@own-team/own-basic-business",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "version": "0.0.314",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -rfwi"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "bumpp": "^9.6.1",
    "eslint": "^9.12.0",
    "esno": "^4.8.0",
    "pnpm": "^9.12.0",
    "rimraf": "^5.0.10",
    "taze": "^0.17.2",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}
