{
  "name": "@own-team/own-basic-business",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "version": "0.0.415",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -rfwi"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@antfu/ni": "^23.2.0",
    "@antfu/utils": "^8.1.0",
    "bumpp": "^9.10.1",
    "eslint": "^9.18.0",
    "esno": "^4.8.0",
    "pnpm": "^9.15.4",
    "rimraf": "^5.0.10",
    "taze": "^18.2.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.7",
    "vitest": "^3.0.2"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}
