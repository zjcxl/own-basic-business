    "bumpp": "^9.10.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.18.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.1",
    "pnpm": "^9.15.4",
    "rimraf": "^5.0.10",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.7",
    "vitest": "^3.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
