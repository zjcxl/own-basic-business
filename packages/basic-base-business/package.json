{
  "name": "@own-basic/business-base",
  "type": "module",
  "version": "0.0.558",
  "packageManager": "pnpm@9.15.4",
  "description": "_description_",
  "author": "chenxueli<zjphchenxueli@163.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjcxl/own-basic-business.git",
    "directory": "packages/basic-base-business"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@antfu/ni": "^23.2.0",
    "@antfu/utils": "^8.1.0",
    "@own-basic-component/config": "^0.8.204",
    "@own-basic-component/request": "^0.8.204",
    "@own-basic-component/util": "^0.8.204",
